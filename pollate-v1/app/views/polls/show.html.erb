<p id="notice"><%= notice %></p>

<p>
  <strong>Title:</strong>
  <%= @poll.title %>
</p>

<div>
  <p>Users in this poll:</p>
  <ul>
    <% @poll.users.each do |user| %>
      <li><a href="/users/<%= user.id %>"><%= user.username %></a></li>
    <% end %>
  </ul>
</div>

<%= link_to 'Edit', edit_poll_path(@poll) %> |
<%= link_to 'Back', polls_path %>
